float distance(vec3 a, vec3 b)
{
    vec3 diff = vec3(a.x - b.x, a.y - b.y, a.z - b.z);
    print(diff.x * diff.x, diff.y * diff.y, diff.z * diff.z);
    float tmp = (diff.x * diff.x) + (diff.y * diff.y) + (diff.z * diff.z);
    return sqrt(tmp);
};

vec3 main()
{
    float dist = distance(vec3(2, 3, 2), vec3(0, 0, 0));
    print(dist);
    return vec3(uv.y, uv.x, uv.x * 0.5);
};
